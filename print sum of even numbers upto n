#include <stdio.h>

int main() {
  
    int n,i,sum,flag;
    scanf("%d",&n);
    i=1;
    sum=0;
    flag=0;
    while(i<=n)
    {
        if(i%2==0)
        {
            if(flag==0){
            printf("%d",i);
            flag=1;
            }
            else {
                printf("+%d",i);
            }
        sum=sum+i;
        }
           i++;
    }
        printf("=%d",sum);
    return 0;
}
